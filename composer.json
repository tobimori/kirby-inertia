{
  "name": "tobimori/kirby-inertia",
  "description": "Kirby CMS adapter for Inertia.js",
  "type": "kirby-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/tobimori/kirby-inertia#readme",
  "authors": [
    {
      "name": "Tobias MÃ¶ritz",
      "email": "tobias@moeritz.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "tobimori\\Inertia\\": "classes"
    }
  },
  "require": {
    "php": ">=8.3.0",
    "getkirby/composer-installer": "^1.2.1"
  },
  "require-dev": {
    "getkirby/cms": "^4.0"
  },
  "scripts": {
    "dist": "composer install --no-dev --optimize-autoloader"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false
  }
}
